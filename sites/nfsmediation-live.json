{
  "name": "NFS Mediation (Live)",
  "baseUrl": "https://nfs.atelierdev.uk",
  "testPages": [
    "/",
    "/about-us/",
    "/category/notices/highlights/",
    "/category/notices/updates/",
    "/category/uncategorised/",
    "/contact-us/",
    "/cookie-settings/",
    "/get-involved/",
    "/meet-the-team/",
    "/notices/",
    "/our-services/",
    "/our-services/asb-assessment-service/",
    "/our-services/coaching-plus-service/",
    "/our-services/cpd-accredited-training-services/",
    "/our-services/hoarding-psychological-support-service/",
    "/our-services/mediation-services/",
    "/our-services/workplace-mediation-services/",
    "/privacy-policy-2/",
    "/sample-page/",
    "/terms-of-use/",
    "/the-team/",
    "/working-with-us/"
  ],
  "discover": {
    "strategy": "sitemap",
    "sitemapUrl": "https://nfs.atelierdev.uk/wp-sitemap.xml",
    "maxPages": 60,
    "maxDepth": 3,
    "include": [
      "^/"
    ]
  },
  "forms": [
    {
      "name": "Contact Form",
      "page": "/contact-us/",
      "selector": ".wpcf7-form, #contact-form, .contact-form",
      "fields": {
        "name": "input[name*='name'], input[name*='your-name']",
        "email": "input[name*='email'], input[name*='your-email']",
        "message": "textarea[name*='message'], textarea[name*='your-message']"
      },
      "submitButton": "input[type='submit'], .wpcf7-submit, button[type='submit']"
    }
  ],
  "criticalElements": [
    {
      "name": "Main Navigation",
      "selector": ".main-navigation, #main-menu, nav, .navigation"
    },
    {
      "name": "Header",
      "selector": "header, .site-header, .header"
    },
    {
      "name": "Footer",
      "selector": "footer, .site-footer, .footer"
    }
  ],
  "expectedElements": {
    "homepage": [
      ".hero-section, .banner, .intro, .home-content"
    ],
    "contact": [
      ".wpcf7-form, #contact-form, .contact-form"
    ],
    "about": [
      ".about-content, .about"
    ]
  }
}

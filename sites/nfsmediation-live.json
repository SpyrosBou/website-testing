{
  "name": "NFS Mediation (Live)",
  "baseUrl": "https://nfs.atelierdev.uk",
  "testPages": [
    "/",
    "/about-us/",
    "/meet-the-team/",
    "/contact-us/",
    "/cookie-settings/",
    "/get-involved/",
    "/our-services/",
    "/services/mediation-services/",
    "/services/workplace-mediation-services/",
    "/services/coaching-plus-service/",
    "/services/asb-assessment-service/",
    "/services/cpd-accredited-training-services/",
    "/services/hoarding-psychological-support-service/",
    "/services/community-mediation/",
    "/notices/",
    "/working-with-us/",
    "/privacy-policy-2/"
  ],
  "forms": [
    {
      "name": "Contact Form",
      "page": "/contact-us/",
      "selector": ".wpcf7-form, #contact-form, .contact-form",
      "fields": {
        "name": "input[name*='name'], input[name*='your-name']",
        "email": "input[name*='email'], input[name*='your-email']",
        "message": "textarea[name*='message'], textarea[name*='your-message']"
      },
      "submitButton": "input[type='submit'], .wpcf7-submit, button[type='submit']"
    }
  ],
  "criticalElements": [
    {
      "name": "Main Navigation",
      "selector": ".main-navigation, #main-menu, nav, .navigation"
    },
    {
      "name": "Header",
      "selector": "header, .site-header, .header"
    },
    {
      "name": "Footer",
      "selector": "footer, .site-footer, .footer"
    }
  ],
  "expectedElements": {
    "homepage": [
      ".hero-section, .banner, .intro, .home-content"
    ],
    "contact": [
      ".wpcf7-form, #contact-form, .contact-form"
    ],
    "about": [
      ".about-content, .about"
    ]
  }
}
